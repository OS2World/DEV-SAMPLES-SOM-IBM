#ifndef dbdog_idl
#define dbdog_idl

#include <ddog.idl>

interface dBigDog : dDog
{
  attribute double BDweight;
  // Weight of dog in pounds.

#ifdef __SOMIDL__
  implementation {
    releaseorder: _get_BDweight, _set_BDweight; 

    //# Class Modifiers
    callstyle = idl;
    dllname = "danimal.dll";
    factory = AnimalFactory;

    memory_management = corba;

    //# Method Modifiers
    somDefaultInit: override;
    display: override;
  };
#endif /* __SOMIDL__ */

};
#endif  /* dbdog_idl */
